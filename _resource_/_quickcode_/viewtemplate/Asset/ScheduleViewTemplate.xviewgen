this.lfn_GetScheduleScript = function (sViewNm)
{
	var sScript = "";
	
	if (this.lfn_IsNull(this.fvDateColId))		return "";
	
 	sScript += "this.lfvDateColId = \"" + this.fvDateColId + "\";";
 	sScript += "this.lfvCalImage = \"" + this.CALENDAR_IMAGE + "\";";
	sScript += "this.lfn_MakeInnerData = function(oDataset)";
	sScript += "{";
	sScript += "	var oDsSchedule = oDataset.parent." + this.CALENDAR_DS_ID	 + ";";
	sScript += "	oDsSchedule.clearData();";
	sScript += "	oDataset.set_enableevent(false);";
	sScript += "	oDataset.filter('');";
	sScript += "	oDataset.set_keystring('S:' + this.lfvDateColId);";
	sScript += "	var sDateCur, sDatePre;";
	sScript += "	var nARow;";
	sScript += "	for(var i = 0; i < oDataset.rowcount; i++)";
	sScript += "	{";
	sScript += "		sDateCur = oDataset.getColumn(i,this.lfvDateColId);";
	sScript += "		if (sDateCur == sDatePre)		continue;";
	sScript += "		nARow = oDsSchedule.addRow();";
	sScript += "		oDsSchedule.setColumn(nARow,'" + this.ARR_INNER_FIELD [0]["id"] + "',sDateCur);";
	sScript += "		oDsSchedule.setColumn(nARow,'" + this.ARR_INNER_FIELD [1]["id"] + "',this.lfvCalImage);";
	sScript += "		sDatePre = sDateCur;";
	sScript += "	}";
	sScript += "	oDataset.set_enableevent(true);";
	sScript += "};";
	sScript += "this.lfn_SetList = function(sDate, obj)";
	sScript += "{";
	sScript += "	var oDataset = obj.parent.viewdataset;";
	sScript += "	oDataset.filter('');";
	sScript += "	if (sDate != null)";
	sScript += "		oDataset.filter(this.lfvDateColId + '==' + nexacro.wrapQuote(sDate));";
	sScript += "};";
	sScript += "this.lfn_Schedule_viewdataset_onrowsetchanged = function(obj,e)";
	sScript += "{";
	//sScript += "	trace('lfn_Schedule_viewdataset_onrowsetchanged : ' + this." + this.CALENDAR_ID + ".value);";
	sScript += "	if (e.reason < 30) {";
	sScript += "		this.lfn_MakeInnerData(obj);";
	sScript += "		this.lfn_SetList(this." + this.CALENDAR_ID + ".value, obj);";
	sScript += "	}";
	sScript += "};";
	sScript += "this.lfn_Schedule_Calendar_onchanged = function(obj,e)";
	sScript += "{";
	//sScript += "	trace('lfn_Schedule_Calendar_onchanged : ' + e.postvalue);";
	sScript += "	this.lfn_SetList(e.postvalue, obj);";
	sScript += "};";
	sScript += "this.lfn_GetDate = function()";
	sScript += "{";
	sScript += "	var d = new Date();";
	sScript += "	var s = d.getFullYear() + ((d.getMonth() + 1) + '').padLeft(2, '0') + (d.getDate() + '').padLeft(2, '0');";
	sScript += "	return s;";
	sScript += "};";
	sScript += "this.viewdataset.addEventHandler('onrowsetchanged', this.lfn_Schedule_viewdataset_onrowsetchanged, this);";
	sScript += "this." + this.CALENDAR_ID + ".addEventHandler('onchanged', this.lfn_Schedule_Calendar_onchanged, this);";
	sScript += "this." + this.CALENDAR_ID + ".set_value(this.lfn_GetDate());";
	
	return sScript;
};
	

// /*---------------------------------------------------------------------------------------------*
//  * Function     : lfn_Grid_onheadclick
//  * Parameter    :
