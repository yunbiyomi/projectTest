this.lfn_Grid_onheadclick = function (sViewNm, sHeadDisplayType, sCheckboxIndex)
{
	var sScript = "";
	
		sScript += "";
		sScript += "var sCheckboxIndex = [];";
		sScript += "var nCount = this.grdList.getCellCount('head');";
		sScript += "for(var i=(nCount-1); i>=0; i--) {";
		sScript += "	var sHeadDisplayType = this.grdList.getCellProperty( 'head', i, 'displaytype' );";
		sScript += "	if (sHeadDisplayType.indexOf('checkboxcontrol') == 0)";
		sScript += "	{";
		sScript += "		sCheckboxIndex.push(i);	";
		sScript += "	}";
		sScript += "}";
		sScript += "this.lfn_Grid_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)";
		sScript += "{";
		sScript += "	for(var i=0; i<sCheckboxIndex.length; i++) ";
		sScript += "	{";
		sScript += "		if (e.col == sCheckboxIndex[i])" ;
		sScript += "		{";
		sScript += "			var objDs = obj.getBindDataset();" ;
		sScript += "			var colID = obj.getCellProperty('body',e.cell,'text').replace('bind:', '');";
		sScript += "			this.CHK_CHECK_ALL = (this.CHK_CHECK_ALL ? 0 : 1);";
		sScript += "			for(let i=0; i<objDs.getRowCount(); ++i) {" ;
		sScript += "				objDs.setColumn(i, colID, this.CHK_CHECK_ALL);"; 
		sScript += "			}" ;
		sScript += "				obj.setCellProperty('head', e.col ,'text',this.CHK_CHECK_ALL);"; 
		sScript += "		}" ;
		sScript +="		}" ;
		sScript +="};"; 

	return sScript;
	
};]]></Script>
